{
  "name": "jspdf-corejs-demo",
  "private": true,
  "homepage": "https://pustovalov.dev",
  "scripts": {
    "fix": "yarn run prettier \"{src,webpack}/**/*.{js,scss,css,svelte}\" --write",
    "clean": "rm -rf build",
    "build": "NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:debug": "NODE_DEBUG=true yarn build",
    "dev": "webpack serve --config webpack/webpack.config.dev.js",
    "dev:debug": "NODE_DEBUG=true yarn dev",
    "sm": "source-map-explorer build/js/*.js",
    "upd": "yarn upgrade-interactive",
    "size": "yarn size-limit",
    "test": "jest src",
    "cy:run": "cypress run",
    "cy:open": "cypress open"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/svelte": "^3.0.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "caniuse-lite": "^1.0.30001174",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.3",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.0.3",
    "sass": "^1.32.2",
    "sass-loader": "^10.1.0",
    "size-limit": "^4.9.1",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^2.0.0",
    "svelte": "^3.31.2",
    "svelte-jester": "^1.3.0",
    "svelte-loader": "^2.13.4",
    "webpack": "^5.12.3",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "core-js": "^3.8.2",
    "jspdf": "^2.2.0"
  },
  "resolutions": {
    "caniuse-lite": "^1.0.30001158"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
